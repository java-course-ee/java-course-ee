<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.javacourse.hibernate">
    
    <class name="Region" table="jc_region">
        <id column="region_id" name="regionId">
            <generator class="native"/>
        </id>
        <property column="region_name" name="regionName"/>
    </class>
    
</hibernate-mapping>
